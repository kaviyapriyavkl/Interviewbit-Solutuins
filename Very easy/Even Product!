public class Solution {
    public int solve(ArrayList<Integer> A) {
        int size=A.size();
    
      if(A.size()==1){return 1;}
      else if(A.size()==2){return 3;}
      double count=1;
      for(int i=0;i<size;i++)
      {
          count=count*2;
          count=count%(int)(1e9+7); 
        }
    return (int)count-1;}
}
